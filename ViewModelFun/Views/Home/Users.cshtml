@model List<User>

<div class="container text-center">
    <div class="row justify-content-center">
        <div class="col">
            <a href="/" class="btn btn-primary">Message</a>
        </div>
        <div class="col">
            <a href="/numbers" class="btn btn-primary">Numbers</a>
        </div>
        <div class="col">
            <a href="/user" class="btn btn-primary">User</a>
        </div>
        <div class="col">
            <a href="/users" class="btn btn-primary">Users</a>
        </div>
    </div>
    <h1 class="mt-5">Here is a list of Users</h1>
    <div class="row justify-content-center">
        <div class="col">
            @foreach(User u in Model)
            {
                <h3>@u.FirstName @u.LastName</h3>
            }
        </div>
    </div>
</div>
