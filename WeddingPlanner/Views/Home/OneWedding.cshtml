
@model Wedding
<div class="row justify-content-between mb-4">
    
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-Between" id="navbarSupportedContent">
                        <h1>Welcome @ViewBag.LogedInUser.FirstName! </h1>
                        <a href="/" class="nav-link">Home</a>
                    </li>
                    <li class="nav-item">
                        <a href="/Weddings/new"class="nav-link">Plan a Wedding</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Logout">Logout</a>
                    </li>
                </ul>
            </div>
        </nav>
    </div>
<div class="container">
    <div class="row justify-content-center mb-4">
        <div class="col text-center">
            <h1 class="h2">Plan a Wedding </h1>
            <hr> <!-- Ligne -->
        </div>
    </div>
<h1>@Model.WedderOne & @Model.WedderTwo</h1>

<h4 class="mt-5">Date: @Model.Date</h4>

<h3 class="mt-5">Guests:</h3>

@foreach (var gg in Model.Guests)
{
    <ul>
        <li>@gg.UserGuest.FirstName @gg.UserGuest.LastName</li>
    </ul>
}
